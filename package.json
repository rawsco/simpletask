{
  "name": "task-manager-app",
  "version": "1.0.0",
  "description": "Serverless task manager application with AWS SAM",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "sam:build": "sam build --template-file app-template.yaml",
    "sam:deploy": "sam deploy --guided",
    "sam:validate": "sam validate --template-file app-template.yaml"
  },
  "keywords": [
    "task-manager",
    "serverless",
    "aws-sam",
    "lambda"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cloudwatch-logs": "^3.1000.0",
    "@aws-sdk/client-dynamodb": "^3.1000.0",
    "aws-lambda": "^1.0.7",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6",
    "@types/uuid": "^9.0.7",
    "aws-sdk": "^2.1693.0",
    "fast-check": "^3.15.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
